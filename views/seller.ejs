<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>SELLER</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
            crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css" rel="stylesheet">

    <link rel="stylesheet" href="css/style.css"/>
    <link rel="stylesheet" href="css/market.css"/>
    <link rel="stylesheet" href="css/categoryList.css"/>
</head>
<body>

<!--HOME _PAGE-->
<!--NAV _BAR-->
<nav class="homeTitle-min navColor nav-height" style="padding-top: 6px">
    <div class="container-fluid">
        <a class="homeTitle-mg navbar-brand navbarX hideNavText" href="/home">SEASON GREEN</a>
        <a class="homeTitle-mg navbar-brand navbarX hideNavText2 showText2" href="/home">SELLER</a>
    </div>
</nav>

<div class="hideNavText2 showHiddenBar">
    <a class=" showHiddenBar" href="/home">CONTROL CENTER</a>
</div>

<div class="row" style="position: relative; left: 12px">
    <div class="row " style="height: 80vh;">


        <div class="marketSideBar col-xl-3 col-md-4 col-12">

            <div class=" sidePst" style="text-align: center">
                <form>

                    <button type="submit" class="hiddenText market-cat market categoryBtn" name="select" value="ALL" >
                        SELLER LIST
                    </button>

                </form>

                <% for(let i = 0 ; i < 2; i ++){ %>


                    <form action="/arrange" method="post">
                        <button type="submit" class="categoryListPst category_box-lg category_box" style="text-transform: uppercase;" name="select" value="" >

                        </button>
                    </form>


                <% } %>

            </div>

        </div>



        <div class=" col-xl-9 col-md-8 ">

            <div class="list-nav tp-margin mrg-l" >

                <div style="display: flex">
                    <p class="hideAddItemBTN col-lg-10 col-md-9 col-sm-8 col-8" style="font-size: 2rem; font-weight: bold">SELLER</p>


                </div>

                <div style="height: 70vh;  overflow: scroll">

                    <% for(let i = 0 ; i < 30; i ++){ %>

                        <div class=" row outline list-box-lg list-box-md list-box-sm" style="border-color: #fd0d0d">

                            <div class=" backgroundCir col-lg-1 col-md-1 col-sm-2 col-2">T</div>

                            <p class=" col-lg-2 col-md-2 col-sm-2 col-2" style="text-align: left; padding-left: 12px">ID: </p>

                            <div class=" col-lg-9 col-md-9 col-sm-6 col-7" style="display: flex" >
                                <div style="text-align: left; width: 50%">SELLER NAME </div>
                                <div  style="text-align: right; width: 50%">DATE</div>
                            </div>


                        </div>

                    <% } %>
                </div>
            </div>
        </div>

    </div>


    <!--            CONTROL PANEL-->



</div>

</body>
</html>
