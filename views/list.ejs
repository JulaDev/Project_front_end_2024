<!-- list.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Snack Items</title>
</head>
<body>
    <h1>List of Snack Items</h1>
    <ul>
        <% items.forEach(function(item) { %>
            <li><%= item.name %></li>
        <% }); %>
    </ul>
</body>
</html>
